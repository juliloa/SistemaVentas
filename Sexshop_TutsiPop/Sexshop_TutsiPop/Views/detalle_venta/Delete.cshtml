@model Sexshop_TutsiPop.Models.detalleventaInfo

@{
    ViewData["Title"] = "Delete";
    Layout = null;
}
    <head>
        <link rel="stylesheet" href="~/css/Styledetails.css" />

    </head>

    <h3>Esta seguro de elimiar esto?</h3>
    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.id_detalle)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.id_detalle)
            </dd>

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.id_venta)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.id_venta)
            </dd>

